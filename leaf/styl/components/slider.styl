.slider
  overflow:hidden
  position:relative
  .stripe
    clearfix()
    .slide
      background-size:cover
      background-position:center center
      height:100%
  .slider-nav
    position:absolute
    bottom:20px
    left:0
    right:0
    width:100%
    text-align:center
    z-index:10
    ul
      margin:0
      li
        padding:0